{
    "check-coverage": true,
    "per-file": true,
    "lines": 90,
    "statements": 90,
    "functions": 90,
    "branches": 90,
    "include": [
        "src/**/*.ts"
    ],
    "exclude":[
        "**/*.d.ts"
    ],
    "extension":[
        ".ts"
    ],
    "reporter": [
        "text-summary",
        "html"
    ],
    "sourceMap": true,
    "instrument": true,
    "all": true
}